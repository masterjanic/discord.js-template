{
  "name": "discord.js-template",
  "version": "1.2.2",
  "description": "A discord.js template for v13 bots featuring slash commands, linter and more.",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "start:pm2": "pm2 start src/index.js --name YOUR_BOT_NAME --max-memory-restart 1G --wait-ready --listen-timeout 10000"
  },
  "keywords": [
    "discord.js",
    "discord bot",
    "discord template",
    "djs"
  ],
  "author": "masterjanic",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterjanic/discord.js-template.git"
  },
  "bugs": {
    "url": "https://github.com/masterjanic/discord.js-template/issues"
  },
  "homepage": "https://github.com/masterjanic/discord.js-template#readme",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.20.0"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "common-tags": "^1.8.0",
    "discord.js": "^13.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  }
}
